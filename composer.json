{
    "name": "nobrain-dev/poc-s4-rabbit",
    "description": "poc: s4 rabbit integration with messenger component",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "php-amqplib/rabbitmq-bundle": "^1.14",
        "prooph/common": "^4.3",
        "symfony/console": "4.3.*",
        "symfony/dotenv": "4.3.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "4.3.*",
        "symfony/messenger": "4.3.*",
        "symfony/monolog-bundle": "^3.4",
        "symfony/yaml": "4.3.*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.15"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "Ms1\\": "poc/Ms1/src/",
            "Ms2\\": "poc/Ms2/src/",
            "BluMS\\": "poc/BluMS/src/",
            "GreenMS\\": "poc/GreenMS/src/",
            "RedMS\\": "poc/RedMS/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/",
            "Ms1\\Tests\\": "poc/Ms1/tests/",
            "Ms2\\Tests\\": "poc/Ms2/tests/",
            "BluMS\\Tests\\": "poc/BluMS/tests/",
            "GreenMS\\Tests\\": "poc/GreenMS/tests/",
            "RedMS\\Tests\\": "poc/RedMS/tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.3.*"
        }
    }
}
